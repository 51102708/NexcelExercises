<?xml version="1.0" encoding="utf-8" ?>
<Functions>
  <Function Key="TenProducts">
    <Setting Key="GetTenMostExpensiveProducts" SpName="Ten Most Expensive Products" DbName="NORTHWIND">
      <Mapping DataField="TenMostExpensiveProducts" ObjectProperty="ProductName" />
      <Mapping DataField="UnitPrice" ObjectProperty="UnitPrice" />
    </Setting>
  </Function>

  <Function Key="ProductWithCategory">
    <Setting Key="GetProductWithCategory" SpName="Products_GetProductWithCategory" DbName="NORTHWIND">
      <Mapping DataField="ProductID" ObjectProperty="Id" />
      <Mapping DataField="ProductName" ObjectProperty="ProductName" />
      <Mapping DataField="SupplierID" ObjectProperty="SupplierID" />
      <Mapping DataField="QuantityPerUnit" ObjectProperty="QuantityPerUnit" />
      <Mapping DataField="UnitPrice" ObjectProperty="UnitPrice" />
      <Mapping DataField="UnitsInStock" ObjectProperty="UnitsInStock" />
      <Mapping DataField="UnitsOnOrder" ObjectProperty="UnitsOnOrder" />
      <Mapping DataField="ReorderLevel" ObjectProperty="ReorderLevel" />
      <Mapping DataField="Discontinued" ObjectProperty="Discontinued" />
      <Mapping DataField="CategoryID" ObjectProperty="Id" Class="Category" />
      <Mapping DataField="CategoryName" ObjectProperty="CategoryName" Class="Category" />
      <Mapping DataField="Description" ObjectProperty="Description" Class="Category" />
      <Mapping DataField="Picture" ObjectProperty="Picture" Class="Category" />
    </Setting>
  </Function>

  <Function Key="Products">
    <Setting Key="GetAllProducs" SpName="Products_GetAllProducts" DbName="NORTHWIND">
      <Mapping DataField="ProductID" ObjectProperty="Id" />
      <Mapping DataField="ProductName" ObjectProperty="ProductName" />
      <Mapping DataField="SupplierID" ObjectProperty="SupplierID" />
      <Mapping DataField="CategoryID" ObjectProperty="CategoryID" />
      <Mapping DataField="QuantityPerUnit" ObjectProperty="QuantityPerUnit" />
      <Mapping DataField="UnitPrice" ObjectProperty="UnitPrice" />
      <Mapping DataField="UnitsInStock" ObjectProperty="UnitsInStock" />
      <Mapping DataField="UnitsOnOrder" ObjectProperty="UnitsOnOrder" />
      <Mapping DataField="ReorderLevel" ObjectProperty="ReorderLevel" />
      <Mapping DataField="Discontinued" ObjectProperty="Discontinued" />
    </Setting>
  </Function>

  <Function Key="ProductAndCategory">
    <Setting Key="GetAllProductsAndCategories" SpName="Products_GetAllProductsAndCategories" DbName="NORTHWIND">
      <Mapping DataField="ProductID" ObjectProperty="Id" Class="Product" />
      <Mapping DataField="ProductName" ObjectProperty="ProductName" Class="Product" />
      <Mapping DataField="SupplierID" ObjectProperty="SupplierID" Class="Product" />
      <Mapping DataField="CategoryID" ObjectProperty="CategoryId" Class="Product" />
      <Mapping DataField="QuantityPerUnit" ObjectProperty="QuantityPerUnit" Class="Product" />
      <Mapping DataField="UnitPrice" ObjectProperty="UnitPrice" Class="Product" />
      <Mapping DataField="UnitsInStock" ObjectProperty="UnitsInStock" Class="Product" />
      <Mapping DataField="UnitsOnOrder" ObjectProperty="UnitsOnOrder" Class="Product" />
      <Mapping DataField="ReorderLevel" ObjectProperty="ReorderLevel" Class="Product" />
      <Mapping DataField="Discontinued" ObjectProperty="Discontinued" Class="Product" />

      <Mapping DataField="CategoryID" ObjectProperty="Id" Class="Category" />
      <Mapping DataField="CategoryName" ObjectProperty="CategoryName" Class="Category" />
      <Mapping DataField="Description" ObjectProperty="Description" Class="Category" />
      <Mapping DataField="Picture" ObjectProperty="Picture" Class="Category" />
    </Setting>
  </Function>
</Functions>